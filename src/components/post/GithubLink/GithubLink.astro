---
import GithubLinkReact from "./GithubLinkReact";

interface Props {
  url: string;
  expanded?: boolean;
  maxHeight?: number;
}

const { url, expanded, maxHeight } = Astro.props;
const hasChildren = Astro.slots.has("default");
---

<GithubLinkReact
  client:load
  url={url}
  expanded={expanded}
  maxHeight={maxHeight}
>
  {hasChildren && <slot />}
</GithubLinkReact>
