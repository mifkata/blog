---
interface Props {
  size?: number;
  class?: string;
}
const { size = 12, class: className } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={size}
  height={size}
  viewBox="0 0 24 24"
  fill="currentColor"
  class={className}
  aria-hidden="true"
>
  <circle cx="12" cy="12" r="10"></circle>
</svg>
